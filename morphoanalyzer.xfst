#define IS [ i s ];

# -----------------------------------------------------------
echo >>> rules for verbs

define VERBPOS %+VB:0;

define VERBROOT [ 0 | %+VBZ:s | %+VBD:{ed} | %+VBG:{ing}];

define VERBCAT VERB VERBPOS VERBROOT;

define VERBRULE1 e -> 0 || e _ d;

define VERBRULE2 [] -> t || t _ e d;

define VERBRULE3 e -> 0 || ?* _ i n g;

define VERBRULE4 0 -> p || p _ e d;

define IRREGVERBS [ {throw}:{threw} | {give}:{gave} | {say}:{said} | {lead}:{led} | {draw}:{drew} | {win}:{won} | {do}:{did} | {tell}:{told} | {eat}:{ate} | {cut}:{cut} ] %+VERB:0 %+VBD:0;

define IRREGVERBSPP [ {be}:{been} | {throw}:{thrown} | {say}:{said} | {give}:{given} | {lead}:{led} | {draw}:{drawn} | {win}:{won} | {do}:{done} | {tell}:{told} | {eat}:{eaten} | {cut}:{cut} ] %+VERB:0 %+VBN:0; 

define TOBE3 [{be}:{is}] %+VB:0 %+VBZ:0;

define TOBEP [{be}:{were}] %+VB:0 %+VBD:0;

define TOBEP13 [{be}:{was}] %+VB:0 %+VBD:0;

define TOBE1 [{be}:{am}] %+VB:0 %+VBF:0;

define TOBE2 [{be}:{are}] %+VB:0 %+VB2P:0;

define TOBEING [{be}:{being}] %+VB:0 %+VBG:0;

define DON [{do}:{don}] %+VB:0 %+VBN:0;

define HAVE3 [{have}:{has}] %+VB:0 %+VBZ:0;

define HAVEP [{have}:{had}] %+VB:0 %+VBD:0;

define TOGO [{go}:{went}] %+VB:0 %+VBD:0;

read regex VERBCAT .o. VERBRULE1 .o. VERBRULE2 .o. VERBRULE3 .o. VERBRULE4;

read regex IRREGVERBS;

read regex IRREGVERBSPP;

read regex TOBE3;

read regex TOBEP;

read regex TOBEP13;

read regex TOBE1;

read regex TOBE2;

read regex TOBEING;

read regex DON;

read regex HAVE3;

read regex HAVEP;

read regex TOGO;

# -----------------------------------------------------------
echo >>> rules for nouns

#define NOUNPOS %+N:0;

define NOUNRULE [[%+NOUN %+SG]:0 | [%+NOUN %+PL]:s];

define NOUNCAT NOUN NOUNRULE;

define NOUNRULE1 y -> i || ?* _ e s .#.;

define PROPERTY [{property}:{properties} | {authority}:{authorities}] [%+NOUN %+SG]:0 [%+NOUN %+PL]:0;

define PEOPLE [{person}:{people}] [%+NOUN %+SG]:0 [%+NOUN %+PL]:0;

define FISH [{fish}:{fish}] [%+NOUN %+SG]:0 [%+NOUN %+PL]:0;

read regex NOUNCAT .o. NOUNRULE1;

read regex PROPERTY;

read regex PEOPLE;

read regex FISH;

#----------------------------------------------------------
echo >>> adjectives' time

define ADJECTIVEROOT [ 0 | %+COMP:{er} | %+SUPER:{est} ];

define ADJECTIVERULE1 e -> 0 || ?* e _ r;

define ADJECTIVERULE2 e -> 0 || ?* e _ s t;

define ADJECTIVECAT ADJECTIVE ADJECTIVEROOT;

read regex ADJECTIVECAT .o. ADJECTIVERULE1 .o. ADJECTIVERULE2;

#----------------------------------------------------------

define GENITIVE [ s ] %+GEN:0;

read regex GENITIVE;

#read regex WORDS;

read regex ADVERB;

read regex NUMERAL;

read regex ORDNUM;

read regex DETERMINER;

read regex PREPOSITION;

read regex CONJUNCTION;

read regex PRONOUN;

read regex RELATIVEPRONOUN;

read regex MODALV;

read regex MODALP;

read regex PUNCT;

union net


